{
    "manifest_version": 2,
    "name": "Input EX",
    "version": "0.1",
    "description": "input拡張ツール",
    "permissions": [
        "storage",
        "tabs",
        "<all_urls>"
    ],
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "content_scripts": [
        {
            "run_at": "document_end",
            "matches":["<all_urls>"], 
            "js": ["jquery-3.5.1.min.js", "content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "logo.png",
        "default_title": "Input Extension"
    }
}